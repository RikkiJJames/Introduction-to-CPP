// Swap

using std::cout;
using std::endl;


void swap(int *a, int *b) {
    int temp = *a;
    *a = *b;
    *b = temp;
}


int main() {
    int x {100}, y {200};
    cout << "x: " << x <<  endl; // Prints 100
    cout << "y: " << y <<  endl; // Prints 200
    
    swap(&x, &y);
    
    cout << "\nx: " << x <<  endl; // Prints 200
    cout << "y: " << y <<  endl; // Prints 100
    
    cout  << endl;
    
    return 0;
}
