#include <iostream>


using std::cout;
using std::cin;
using std::endl;


int find_area(int x);
double find_area(double x, double y);

int main(){
    
    int square_area = find_area(2);
    double rectangle_area = find_area(4.5,2.3);
    
    cout << "The area of the square is " << square_area // Prints 4
         << "\n" << "The area of the rectangle is " << rectangle_area << endl; // Prints 10.35
    
    return 0;
    
}

int find_area(int x){    
    return x*x;
}

double find_area(double x, double y){    
    return x*y;
}



